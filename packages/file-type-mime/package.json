{
  "name": "file-type-mime",
  "version": "0.3.5",
  "description": "Utility to parse mime type from a file content",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check": "biome check src",
    "format": "biome format src --write",
    "build": "unbuild",
    "test": "vitest run",
    "execute": "node node-test.js",
    "coverage": "vitest run --coverage",
    "release": "pnpm build && pnpm publish",
    "validate": "pnpm build && pnpm execute"
  },
  "keywords": [
    "mime",
    "file",
    "type",
    "content",
    "content-type",
    "parse",
    "bytes",
    "json"
  ],
  "author": {
    "name": "Edmundas Ramanauskas",
    "email": "edmundas.ramanauskas@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redmundas/file-type-mime.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.5.2",
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^1.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.2.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/index.ts",
      "data",
      "dist"
    ],
    "all": true
  }
}
